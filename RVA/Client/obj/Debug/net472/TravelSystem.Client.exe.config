<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <system.serviceModel>
    <client>
      <endpoint address="net.pipe://localhost/TravelService" binding="netNamedPipeBinding" bindingConfiguration="NetNamedPipeBinding_ITravelManagementService" contract="TravelSystem.Contracts.ServiceContracts.ITravelManagementService" name="TravelManagementEndpoint" />
    </client>
    <bindings>
      <netNamedPipeBinding>
        <binding name="NetNamedPipeBinding_ITravelManagementService" maxReceivedMessageSize="65536" maxBufferSize="65536">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
        </binding>
      </netNamedPipeBinding>
    </bindings>
  </system.serviceModel>
</configuration>